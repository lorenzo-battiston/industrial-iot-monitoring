      "targets": [
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "-- Daily Production Overview: Shows job completion statistics\n-- Requested by professor: \"15 out of 25 jobs completed – 60%\"\nWITH daily_job_stats AS (\n  SELECT \n    -- Count total jobs started today\n    COUNT(DISTINCT m.job_id) as total_jobs_started,\n    -- Count completed jobs (>= 100% progress)\n    COUNT(DISTINCT CASE WHEN m.job_progress >= 1.0 THEN m.job_id END) as completed_jobs,\n    -- Calculate completion percentage\n    ROUND(\n      CAST(\n        COUNT(DISTINCT CASE WHEN m.job_progress >= 1.0 THEN m.job_id END) * 100.0 / \n        NULLIF(COUNT(DISTINCT m.job_id), 0) \n      AS NUMERIC), 1\n    ) as completion_percentage,\n    -- Total units produced today\n    SUM(DISTINCT m.produced_units) as total_units_produced,\n    -- Total target units for today\n    SUM(DISTINCT m.target_units) as total_target_units,\n    -- Average daily efficiency\n    ROUND(\n      CAST(\n        SUM(DISTINCT m.produced_units) * 100.0 / \n        NULLIF(SUM(DISTINCT m.target_units), 0)\n      AS NUMERIC), 1\n    ) as production_efficiency\n  FROM machine_metrics_5min m\n  WHERE \n    -- Only persistent jobs from today\n    m.job_id LIKE 'JOB_%'\n    AND DATE(m.window_start) = CURRENT_DATE\n)\nSELECT \n  'Daily Production Overview' as metric,\n  completion_percentage as value\nFROM daily_job_stats\nUNION ALL\nSELECT \n  'Production Efficiency' as metric,\n  production_efficiency as value\nFROM daily_job_stats;",
          "refId": "A"
        }
      ],
      "title": "Daily Production Overview",
