        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "-- Product Completion Overview: Shows finished products based on bottleneck principle\n-- Products completed = minimum units produced across all machines\nWITH current_jobs AS (\n  -- Get latest data per machine to avoid duplicates\n  SELECT DISTINCT ON (machine_id) \n    machine_id, job_id, target_units, produced_units, job_progress, window_start\n  FROM machine_metrics_5min \n  WHERE job_id LIKE 'JOB_%'\n  ORDER BY machine_id, window_start DESC\n),\nproduct_metrics AS (\n  SELECT \n    -- Product completion based on bottleneck (minimum across machines)\n    MIN(produced_units) as completed_products,\n    ROUND(AVG(target_units)) as avg_target,\n    ROUND(CAST(MIN(produced_units) * 100.0 / AVG(target_units) AS NUMERIC), 1) as product_completion_rate\n  FROM current_jobs\n)\nSELECT \n  'Product Completion Rate' as metric,\n  product_completion_rate as value\nFROM product_metrics;",
          "refId": "A"
        }
      ],
