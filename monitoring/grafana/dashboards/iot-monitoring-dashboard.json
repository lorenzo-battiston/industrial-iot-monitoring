        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "-- Daily Production Summary Table\n-- Shows the exact format requested by professor\nWITH daily_summary AS (\n  SELECT \n    COUNT(DISTINCT m.job_id) as total_jobs,\n    COUNT(DISTINCT CASE WHEN m.job_progress >= 1.0 THEN m.job_id END) as completed_jobs,\n    ROUND(\n      CAST(\n        COUNT(DISTINCT CASE WHEN m.job_progress >= 1.0 THEN m.job_id END) * 100.0 / \n        NULLIF(COUNT(DISTINCT m.job_id), 0) \n      AS NUMERIC), 1\n    ) as completion_percentage,\n    SUM(DISTINCT m.produced_units) as total_produced,\n    SUM(DISTINCT m.target_units) as total_target\n  FROM machine_metrics_5min m\n  WHERE \n    m.job_id LIKE 'JOB_%'\n    AND DATE(m.window_start) = CURRENT_DATE\n)\nSELECT \n  CURRENT_DATE as \"Production Date\",\n  completed_jobs || ' out of ' || total_jobs || ' jobs completed – ' || completion_percentage || '%' as \"Completion Status\",\n  total_produced || ' / ' || total_target || ' units' as \"Production Progress\",\n  CASE \n    WHEN completion_percentage >= 90 THEN '🟢 Excellent'\n    WHEN completion_percentage >= 70 THEN '🟡 Good'\n    WHEN completion_percentage >= 50 THEN '🟠 Fair'\n    ELSE '🔴 Needs Attention'\n  END as \"Performance Rating\"\nFROM daily_summary;",
          "refId": "A"
        }
      ],
      "title": "Daily Production Summary",
      "type": "table"
    },
