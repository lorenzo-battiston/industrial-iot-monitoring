          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH latest AS (\n    SELECT DISTINCT ON (machine_id) machine_id, target_units, produced_units\n    FROM machine_metrics_5min WHERE job_id LIKE 'JOB_%' ORDER BY machine_id, window_start DESC\n) , agg AS (\n    SELECT SUM(target_units) AS planned, SUM(produced_units) AS completed\n    FROM latest\n)\nSELECT 'Products Planned'   AS \"Metric\", planned   AS \"Value\" FROM agg\nUNION ALL\nSELECT 'Products Completed', completed FROM agg\nUNION ALL\nSELECT 'Completion Rate (%)', ROUND((completed*100.0/NULLIF(planned,0))::numeric,1) FROM agg;",
          "refId": "A"
        }
      ],
