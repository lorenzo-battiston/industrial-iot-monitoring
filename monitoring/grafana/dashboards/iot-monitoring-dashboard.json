      "targets": [
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "-- LIVE MACHINE STATUS - Machine Job Progress\nSELECT\n  DISTINCT ON (m.machine_id) \n  m.machine_id as \"Machine ID\",\n  -- Map machines to fixed production stages\n  CASE m.machine_id\n    WHEN 'MACHINE_001' THEN 'Processing'\n    WHEN 'MACHINE_002' THEN 'Assembly'\n    WHEN 'MACHINE_003' THEN 'Quality Check'\n    WHEN 'MACHINE_004' THEN 'Packaging'\n    WHEN 'MACHINE_005' THEN 'Shipping'\n    ELSE 'Unknown'\n  END as \"Current Stage\",\n  -- Job completion percentage\n  ROUND(CAST(m.job_progress * 100 AS NUMERIC), 1) || '%' as \"Job Progress\",\n  -- Performance indicator without emojis\n  CASE \n    WHEN m.job_progress >= 0.95 THEN 'Excellent'\n    WHEN m.job_progress >= 0.8 THEN 'Good'\n    WHEN m.job_progress >= 0.6 THEN 'Fair'\n    ELSE 'In Progress'\n  END as \"Status\",\n  m.target_units as \"Target Units\",\n  m.produced_units as \"Produced Units\",\n  (m.target_units - m.produced_units) as \"Remaining Units\",\n  -- Get scrap data from machine_metrics_5min\n  COALESCE(m.scrap_units, 0) as \"Scrap Units\",\n  ROUND(CAST(COALESCE(m.scrap_rate, 0) * 100 AS NUMERIC), 2) || '%' as \"Scrap Rate\",\n  m.operator_name as \"Operator\",\n  m.shift as \"Shift\",\n  TO_CHAR(m.window_start, 'HH24:MI:SS') as \"Last Update\"\nFROM \n  machine_metrics_5min m\nWHERE m.job_id LIKE 'JOB_%'\nORDER BY \n  m.machine_id, m.window_start DESC;",
          "refId": "A",
          "table": "machine_metrics_5min",
          "timeColumns": [
            "window_start"
          ],
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "LIVE MACHINE STATUS - Machine Job Progress",
      "type": "table"
    },
