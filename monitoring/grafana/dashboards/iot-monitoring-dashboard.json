          "rawSql": "WITH latest AS (\n  SELECT DISTINCT ON (machine_id) machine_id, target_units, produced_units\n  FROM machine_metrics_5min WHERE job_id LIKE 'JOB_%' ORDER BY machine_id, window_end DESC\n), agg AS (\n  SELECT MIN(target_units) planned, MIN(produced_units) completed\n  FROM latest\n)\nSELECT 'Planned Units'   AS Metric, planned   AS Value FROM agg\nUNION ALL\nSELECT 'Completed Units', completed        FROM agg\nUNION ALL\nSELECT 'Completion Rate (%)', ROUND((completed*100.0/NULLIF(planned,0))::numeric,1) FROM agg;",
          "refId": "A"
        }
