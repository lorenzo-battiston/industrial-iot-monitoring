          "rawSql": "WITH latest AS (\n  SELECT DISTINCT ON (machine_id) machine_id, target_units, produced_units, job_progress\n  FROM machine_metrics_5min\n  WHERE job_id LIKE 'JOB_%'\n  ORDER BY machine_id, window_start DESC\n), agg AS (\n  SELECT SUM(produced_units) AS processed, SUM(target_units) AS target,\n         ROUND(AVG(job_progress*100)::numeric,1) AS avg_util\n  FROM latest\n)\nSELECT 'Pipeline Efficiency' AS \"Metric\", ROUND((processed*100.0/NULLIF(target,0))::numeric,1) AS \"Value\" FROM agg\nUNION ALL\nSELECT 'Average Machine Utilisation', avg_util FROM agg;",
