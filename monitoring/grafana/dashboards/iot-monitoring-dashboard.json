        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  -- Enhanced Live Production Status with Job Progress\n  DISTINCT ON (m.machine_id) \n  m.machine_id,\n  m.job_id,\n  m.target_units,\n  m.produced_units,\n  -- Job completion percentage\n  ROUND(CAST(m.job_progress * 100 AS NUMERIC), 1) || '%' as \"Job Progress\",\n  -- Calculate remaining units\n  (m.target_units - m.produced_units) as \"Remaining Units\",\n  -- Enhanced scrap data\n  COALESCE(s.total_scrap, 0) as scrap_units,\n  COALESCE(s.total_good, 0) as good_units,\n  ROUND(CAST(COALESCE(s.avg_scrap_rate, 0) * 100 AS NUMERIC), 2) || '%' as scrap_rate,\n  ROUND(CAST(COALESCE(s.avg_quality_score, 0) AS NUMERIC), 3) as avg_quality_score,\n  COALESCE(s.scrap_incidents, 0) as defect_count,\n  m.operator_name,\n  m.shift,\n  -- Format last update nicely\n  TO_CHAR(m.window_start, 'HH24:MI:SS') as \"Last Update\"\nFROM \n  machine_metrics_5min m\nLEFT JOIN (\n  -- Get latest scrap summary per machine (last 4 hours)\n  SELECT \n    machine_id,\n    SUM(scrap_units) as total_scrap,\n    SUM(good_units) as total_good,\n    AVG(scrap_rate) as avg_scrap_rate,\n    AVG(quality_score) as avg_quality_score,\n    COUNT(*) as scrap_incidents\n  FROM production_scrap \n  WHERE timestamp >= NOW() - INTERVAL '4 hours'\n  GROUP BY machine_id\n) s ON m.machine_id = s.machine_id\n-- Filter to show only persistent jobs (new format)\nWHERE m.job_id LIKE 'JOB_%'\nORDER BY \n  m.machine_id, m.window_start DESC;",
          "refId": "A",
          "table": "machine_metrics_5min",
          "timeColumns": [
            "window_start"
          ],
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Live Production Status (Current Shift)",
      "type": "table"
